<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="1" author="aliaksei.bulhak">
        <rollback changeSetPath="./user[rollback].sql" />
        <sqlFile path="./user.sql" relativeToChangelogFile="true"/>
    </changeSet>

    <changeSet id="2" author="aliaksei.bulhak">
        <sqlFile path="insert.sql" relativeToChangelogFile="true"/>
    </changeSet>

    <changeSet id="3" author="aliaksei.bulhak">
        <tagDatabase tag="1.0.0"/>
    </changeSet>

</databaseChangeLog>
